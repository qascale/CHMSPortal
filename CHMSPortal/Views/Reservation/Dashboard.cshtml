@{
    ViewBag.Title = "Dashboard";
}

<!-- START carousel-->
<div id="carousel-example-captions" data-ride="carousel" class="carousel slide">
    <ol class="carousel-indicators">
        <li data-target="#carousel-example-captions" data-slide-to="0" class="active"></li>
        <li data-target="#carousel-example-captions" data-slide-to="1"></li>        
    </ol>
    <div role="listbox" class="carousel-inner">
        <div class="item active">
            <img src="~/Content/Images/CircuitHouse/JP1.png" alt="First slide image" />           
        </div>
        <div class="item">
            <img src="~/Content/Images/CircuitHouse/JP2.png" alt="Second slide image" />          
        </div>
       
    </div>
    <a href="#carousel-example-captions" role="button" data-slide="prev" class="left carousel-control">
          <span aria-hidden="true" class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
    </a>
    <a href="#carousel-example-captions" role="button" data-slide="next" class="right carousel-control">
          <span aria-hidden="true" class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
    </a>
</div>
<div class="row"></div><br />
   <!-- START widgets box-->
<div class="row">
    <div class="col-lg-4 col-sm-6">
        <!-- START widget-->
        <div class="panel bg-info-light pt b0 widget">
            <div class="ph">
                @*<em class="icon-control-start fa-lg pull-right"></em>*@
                <div class="h2 mt0"></div>
                <div class="text-uppercase">My Booking Status</div>
            </div>
            <div data-sparkline="" data-type="line" data-width="100%" data-height="75px" data-line-color="#23b7e5" data-chart-range-min="0" data-fill-color="#23b7e5" data-spot-color="#23b7e5" data-min-spot-color="#23b7e5" data-max-spot-color="#23b7e5"
                 data-highlight-spot-color="#23b7e5" data-highlight-line-color="#23b7e5" values="2,5,3,7,4,5" style="margin-bottom: -2px" data-resize="true"></div>
        </div>
    </div>
    <div class="col-lg-4 col-sm-6">
        <!-- START widget-->
        <div class="panel widget bg-purple-light pt b0 widget">
            <div class="ph">
                @*<em class="icon-action-undo fa-lg pull-right"></em>*@
                <div class="h2 mt0">
                    
                    <span class="text-sm text-white"></span>
                </div>
                <div class="text-uppercase">Cancel Room Booking</div>
            </div>
            <div data-sparkline="" data-type="line" data-width="100%" data-height="75px" data-line-color="#7266ba" data-chart-range-min="0" data-fill-color="#7266ba" data-spot-color="#7266ba" data-min-spot-color="#7266ba" data-max-spot-color="#7266ba"
                 data-highlight-spot-color="#7266ba" data-highlight-line-color="#7266ba" values="1,4,5,4,8,7,10" style="margin-bottom: -2px" data-resize="true"></div>
        </div>
    </div>
    <div class="col-lg-4 col-md-6 col-sm-12">
        <!-- START widget-->
        <div class="panel widget bg-info-light pt b0 widget">
            <div class="ph">
                @*<em class="icon-printer fa-lg pull-right"></em>*@
                <div class="h2 mt0"></div>
                <div class="text-uppercase">Bill Print</div>
            </div>
            <div data-sparkline="" data-type="line" data-width="100%" data-height="75px" data-line-color="#23b7e5" data-chart-range-min="0" data-fill-color="#23b7e5" data-spot-color="#23b7e5" data-min-spot-color="#23b7e5" data-max-spot-color="#23b7e5"
                 data-highlight-spot-color="#23b7e5" data-highlight-line-color="#23b7e5" values="4,5,3,10,7,15" style="margin-bottom: -2px" data-resize="true"></div>
        </div>
    </div>
   
</div>
    <!-- END carousel-->

   
@section Styles {
    @Styles.Render("~/bundles/jvectormapCss")
}
@section Scripts {
@Scripts.Render("~/bundles/EasyPie")
    @Scripts.Render("~/bundles/sparklines")
    @Scripts.Render("~/bundles/flot")
    @Scripts.Render("~/bundles/jvectormap")
    @Scripts.Render("~/bundles/demoFlot")
    @Scripts.Render("~/bundles/demoVectormap")
    @Scripts.Render("~/bundles/demoDashboard")
}



   
